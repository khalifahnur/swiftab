{
  "expo": {
    "name": "Swiftab",
    "slug": "Swiftab",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/st/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/st/preview.png",
      "resizeMode": "contain",
      "backgroundColor": "#008080"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/st/preview.png",
        "backgroundColor": "#008080"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.lifkha.swiftab"
    },
    "plugins": [
      "expo-router",
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoibGlma2hhIiwiYSI6ImNtMXdpdnVnejBjZTEyanM5aWl5cHdjZnMifQ.NAoD6a6lcDVly8JugZ6TTA"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(SwifTab) to use your location."
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(SwifTab) needs access to your Camera.",
          "enableMicrophonePermission": false,
          "microphonePermissionText": "$(SwifTab) needs access to your Microphone.",
          "enableCodeScanner": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/st/icon.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": false
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "c375945a-3b31-45bf-ac84-caad9905bb27"
      }
    }
  }
}
